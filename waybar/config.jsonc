{
    "layer": "top",
    "position": "top",
    //"gtk-layer-shell": "false",
    "height": 45,
    // "width": 2560,
    "spacing": 0,
    "margin-top": 0,
    //"margin-bottom":-10,
    "margin-left": 10,
    "margin-right": 10,


    "modules-left": [
        "custom/launcher",
        "hyprland/workspaces",
        // "cpu",
        // "memory",
        // "temperature",
        // "disk",
        "custom/updates"
    ],
    "modules-center": [
        "custom/music",
        "custom/lizzy",
        "hyprland/window"
    ],
    "modules-right": [
        "network",
        "bluetooth",
        "pulseaudio",
        "backlight",
        "battery",
        "custom/notifications",
        "tray",
        "clock",
        // "custom/weather",
        // "custom/cycle_wall",
        // "custom/clipboard",
        "custom/power"
        // "custom/custom"
    ],
    "custom/launcher": {
        "format": "{}",
        "tooltip": true,
        "exec": "echo '{\"text\":\"üíß\",\"tooltip\":\"Drun | Run\"}'",
        "return-type": "json",
        "on-click": "pkill rofi || exec ~/.config/rofi/launchers/type-3/launcher.sh",
        //"on-click": "pkill tofi || sleep 0.15 && tofi-drun -c ~/.config/tofi/tofi.launcher.conf",
        "on-click-right": "pkill tofi || sleep 0.15 && tofi-run -c ~/.config/tofi/tofi.launcher.conf",
        // "on-click": "pkill wofi || wofi --show drun -n",
        // "on-click-right": "pkill wofi || wofi --show run -n",
    },
    "hyprland/workspaces": {
         //"format": "{icon} {name}",
        "format": "",
      "format-icons": {
            "active": "ÔÜí",
            "default": "ÔÑë"
        },
        "on-scroll-up": "hyprctl dispatch workspace e-1",
        "on-scroll-down": "hyprctl dispatch workspace e+1",
        "on-click": "activate"
        },
    "cpu": {
        "format": "Ô°ö {usage}%",
        "tooltip": true,
        "on-click": "kitty --start-as=fullscreen --title all_is_kitty sh -c 'btop'",
        "interval": 2
    },
    "memory": {
        "format": "Ôãõ {}%",
        "tooltip": true,
        "on-click": "kitty --start-as=fullscreen --title all_is_kitty sh -c 'btop'",
        "interval": 2
    },
    "temperature": {
        // "thermal-zone": 2,
        // "hwmon-path": "/sys/class/hwmon/hwmon2/temp1_input",
        "critical-threshold": 40,
        "format-critical": "{icon} {temperatureC}¬∞C",
        "format": "{icon} {temperatureC}¬∞C",
        "format-icons": [
            "Ôãâ",
            "Ôãà",
            "Ôãá"
        ],
        "tooltip": true,
        "on-click": "kitty --start-as=fullscreen --title all_is_kitty sh -c 'btop'",
        "interval": 2
    },
    "disk": {
        "format": "ÔÇ† {percentage_used}% ({free})",
        "tooltip": true,
        "on-click": "kitty --start-as=fullscreen --title all_is_kitty sh -c 'btop'",
        "interval": 2
    },
    "custom/updates": {
        "format": "{}",
        "exec": "~/.config/hypr/scripts//updates",
        "on-click": "~/.config/hypr/scripts//updates update",
        "interval": 600,
        "tooltip": true,
        // "tooltip-format": "{}",
        // "exec-tooltip": "~/.config/hypr/scripts//updates tooltip"
    },
    "custom/music": {
        "format": "{icon}{}",
        "format-icons": {
            // "Playing": "ÔÖÑ ", // Uncomment if not using the dynamic script
            "Paused": "Ô£§ ",
            "Stopped": "&#x202d;Ô≠• " // This stop symbol is RTL. So &#x202d; is left-to-right override.
        },
        "escape": true,
        "tooltip": true,
        "exec": "~/.config/hypr/scripts/caway -b 10 -e",
        "return-type": "json",
        "on-click": "playerctl play-pause",
        "on-scroll-up": "playerctl previous",
        "on-scroll-down": "playerctl next",
        "on-click-right": "amberol",
        "max-length": 35
        },
        "custom/lizzy": {
		"format": "<span color='#25be6a'>{icon}</span>  {}",
		"exec": "lizzy --format ' {{title}} - {{artist}}' --autotoggle --mediaplayer spotify",
		"return-type": "json",
		"format-icons": {
		  "Playing": "ÔÜº",
		  "Paused": "ÔÅå"
		},
		"max-length": 45,
		"on-click":"playerctl --player=spotify play-pause",
		"on-scroll-up":"playerctl --player=spotify next",
		"on-scroll-down":"playerctl --player=spotify previous"
	},
    "hyprland/window": {
        "format": "{}",
        "separate-outputs": true,
        "max-length": 35
    },
    "network": {
        // "interface": "wlp2*", // (Optional) To force the use of this interface
        "format": "‚Üï{bandwidthTotalBytes}",
        "format-disconnected": "{icon} No Internet",
        "format-linked": "Ôûñ {ifname} (No IP)",
        "format-alt": "‚Üï{bandwidthUpBytes} | ‚Üï{bandwidthDownBytes}",
        "tooltip-format": "{ifname}: {ipaddr}/{cidr} Ôûñ {gwaddr}",
        "tooltip-format-wifi": "{icon} {essid} ({signalStrength}%)",
        "tooltip-format-ethernet": "{icon} {ipaddr}/{cidr}",
        "tooltip-format-disconnected": "{icon} Disconnected",
        "on-click-right": "nm-connection-editor",
        "on-click":"~/.config/rofi/scripts/rofi-wifi-menu.sh",
        "format-icons": {
            "ethernet": "Ôõø",
            "disconnected": "‚ö†",
            "wifi": [
                "Ô™©",
                "Ô™®"
            ]
        },
        "interval": 2
    },
    "bluetooth":{
    "format-on": "<span color='#e57474'>Ôäî</span>  Turned On",
    "format-off": "<span color='#e57474'>Ôäî</span>  Turned Off",
    "format-disabled": "<span color='#e57474'>Ôäî</span>  Disabled",
    "format-connected": "<span color='#e57474'>Ôäî</span> Connected",
    "format": "<span color='#e57474'>Ôäî</span> Disabled",
        "on-click":"blueman-manager &"
    },
    "pulseaudio": {
        // "scroll-step": 1, // %, can be a float
        "format": "{icon} {volume}%", // {format_source}
        "format-bluetooth": "{icon} {volume}%", // {format_source}
        "format-bluetooth-muted": "Ôüè", // {format_source}
        "format-muted": "Ôë¶", // {format_source}
        "format-source": "{volume}% ÔÑ∞",
        "format-source-muted": "ÔÑ±",
        "format-icons": {
            "headphone": "Ôüä",
            "headset": "Ôüç",
            "phone": "ÔÇï",
            "portable": "ÔÇï",
            "car": "ÔÜπ ",
            "default": [
                "ÔÄ¶",
                "ÔÄß",
                "ÔÄ®"
            ]
        },
        "on-click": "pactl set-sink-mute @DEFAULT_SINK@ toggle",
        "on-click-right": "pavucontrol"
    },
    "backlight": {
        // "device": "acpi_video1",
        //"format": "<span color='#8ccf7e'>Û±©ñ </span>  {percent}%   ",
        "format": "{icon} {percent}%",
        "format-icons": [
            "Óéç",
            "Óèì",
            "Óèë",
            "Óèè",
            "Óèé",
            "Óèç",
            "Óèä",
            "Óèà",
            "Óéõ"
        ],
        "on-scroll-up": "light -A 1",
        "on-scroll-down": "light -U 1",
        "interval": 2
    },
    "battery": {
        "states": {
            "good": 100,
            "warning": 30,
            "critical": 10
        },
        "format": "{icon} {capacity}%",
        "format-charging": "ÔÉß {capacity}%",
        "format-plugged": "Ôá¶ {capacity}%",
        "format-alt": "{icon} {time}",
        // "format-good": "", // An empty format will hide the module
        "format-full": "ÔñÑ {capacity}%",
        "format-icons": [
            "Ôïπ",
            "Ôï∫",
            "Ôïª",
            "Ôïº",
            "ÔïΩ",
            "Ôïæ",
            "Ôïø",
            "ÔñÄ",
            "ÔñÅ",
            "Ôï∏"
        ],
        "interval": 2
    },
    "custom/notifications": {
        "tooltip": false,
        "format": "{icon}",
        "format-icons": {
            "notification": "ÔÇ¢<span foreground='red'><sup>ÔëÑ</sup></span>",
            "none": "ÔÇ¢",
            "dnd-notification": "Ôá∑<span foreground='red'><sup>ÔëÑ</sup></span>",
            "dnd-none": "Ôá∑"
        },
        "return-type": "json",
        "exec-if": "which swaync-client",
        "exec": "swaync-client -swb",
        "on-click": "sleep 0.15 && swaync-client -t -sw",
        "on-click-right": "sleep 0.15 && swaync-client -d -sw",
        "escape": true
    },
    "tray": {
        "icon-size": 15,
        "spacing": 15
    },
    "clock": {
        "timezone": "America/Chicago",
        "format": "ÔÄó {:%d <small>%a</small> %H:%M}",
        //"format": "ÔÄó {:%a %b %d %Y | %H:%M}",
        "format-alt": "ÔÄó {:%A %B %d %Y (%V) | %r}",
        "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
        "calendar-weeks-pos": "right",
        "today-format": "<span color='#f38ba8'><b><u>{}</u></b></span>",
        "format-calendar": "<span color='#f2cdcd'><b>{}</b></span>",
        "format-calendar-weeks": "<span color='#94e2d5'><b>W{:%U}</b></span>",
        "format-calendar-weekdays": "<span color='#f9e2af'><b>{}</b></span>",
        "interval": 60
    },
    "idle_inhibitor": {
        "format": "{icon}",
        "format-icons": {
            "activated": "ÔÅÆ",
            "deactivated": "ÔÅ∞"
        },
        "tooltip": "true"
    },
    "custom/weather": {
        "tooltip": true,
        "format": "{}",
        "exec": "~/.config/hypr/scripts/expand weather",
        "return-type": "json"
    },
    "custom/ss": {
        "format": "{}",
        "exec": "~/.config/hypr/scripts/expand ss-icon",
        "return-type": "json",
        "on-click": "~/.config/hypr/scripts//screenshot_full"
    },
    "custom/cycle_wall": {
        "format": "{}",
        "exec": "~/.config/hypr/scripts/expand wall",
        "return-type": "json",
        // "interval": 1,
        "on-click": "~/.config/hypr/scripts/expand cycle",
        "on-click-right": "~/.config/hypr/scripts/expand cycler"
    },
    "custom/clipboard": {
        "format": "{}",
        "exec": "~/.config/hypr/scripts/expand clipboard",
        "return-type": "json",
        // Window position is managed in Hyperland config's windowrulev2
        "on-click": "pkill tofi || sleep 0.15 && cliphist list | tofi -c ~/.config/tofi/tofi.clip.conf | cliphist decode | wl-copy",
        "on-click-middle": "rm -f ~/.cache/cliphist/db",
        "on-click-right": "pkill tofi || sleep 0.15 && cliphist list | tofi -c ~/.config/tofi/tofi.clip.conf | cliphist delete",
        // Here "-l top_right -x -15 -y 10" doesn't matter as '-n' mode is used
        // "on-click": "pkill wofi || cliphist list | wofi --dmenu -p clippick -l top_right -x -15 -y 10 -n | cliphist decode | wl-copy",
        // "on-click-middle": "rm -f ~/.cache/cliphist/db",
        // "on-click-right": "pkill wofi || cliphist list | wofi --dmenu -p clippick -l top_right -x -15 -y 10 -n | cliphist delete",
        "escape": true
    },
    "custom/power": {
        "format": "{}",
        // "exec": "~/.config/hypr/scripts/expand power",
        "exec": "echo '{\"text\":\"‚èª\",\"tooltip\":\"Power\"}'",
        "return-type": "json",
        //"on-click": "~/.config/wlogout/launch.sh",
        "on-click":"~/.config/rofi/powermenu/type-4/powermenu.sh"
    },
    "custom/custom": {
        "format": "{}",
        "exec": "~/.config/hypr/scripts/expand arrow-icon",
        "on-click": "~/.config/hypr/scripts/expand_toolbar",
        "return-type": "json"
    }
}
